{
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest",
    "test:w": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "pinia": "2.0.0-rc.13",
    "vue": "3.2.20"
  },
  "devDependencies": {
    "@pinia/testing": "0.0.3",
    "@testing-library/vue": "6.5.0",
    "@vitejs/plugin-vue": "1.9.3",
    "@vue/compiler-sfc": "3.2.20",
    "@vue/test-utils": "2.0.0-rc.15",
    "esbuild": "0.13.8",
    "esbuild-jest": "0.5.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "sass": "1.43.2",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4",
    "vite": "2.6.10",
    "vite-plugin-windicss": "1.4.12",
    "vue3-jest": "27.0.0-alpha.2",
    "windicss": "3.1.9"
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": [
        "esbuild-jest",
        {
          "sourcemap": true,
          "loaders": {
            ".spec.ts": "tsx"
          }
        }
      ],
      ".*\\.(vue)$": "vue3-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/style-mock.js",
      "shared/(.*)": "<rootDir>/src/shared/$1"
    }
  }
}
